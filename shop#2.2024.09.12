#include <stdio.h>

int main(void) {
    int n;
    int in[101], sold[101], remain[101];
    int i;

    scanf("%d", &n);

    for (i = 1; i <= n; i++)
        scanf("%d", &in[i]);

    for (i = 1; i <= n; i++)
        scanf("%d", &sold[i]);

    for (i = 1; i <= n; i++)
        remain[i] = in[i] - sold[i];

    int totalSold = 0;
    for (i = 1; i <= n; i++)
        totalSold += sold[i];

    int maxID = 1, minID = 1;
    for (i = 2; i <= n; i++) {
        if (sold[i] > sold[maxID]) maxID = i;
        if (sold[i] < sold[minID]) minID = i;
    }

    printf("재고수량 : ");
    for (i = 1; i <= n; i++) {
        printf("%d", remain[i]);
        if (i < n) printf(" ");
    }
    printf("\n");

    double percent = 0;
    int totalIn = 0;
    for (i = 1; i <= n; i++)
        totalIn += in[i];

    if (totalIn > 0)
        percent = (double)totalSold / totalIn * 100.0;

    printf("총 판매량 : %d (판매율 %.2f%%)\n", totalSold, percent);

    printf("가장 많이 판매된 상품 : ID %d, 판매량 %d\n", maxID, sold[maxID]);
    printf("가장 적게 판매된 상품 : ID %d, 판매량 %d\n", minID, sold[minID]);

    for (i = 1; i <= n; i++) {
        if (remain[i] == 2)
            printf("상품 ID %d : 재고부족(2)\n", i);
        else if (remain[i] <= 1)
            printf("상품 ID %d : 재고부족(%d)\n", i, remain[i]);
    }

    return 0;
}
